# Top-most EditorConfig file
root = true

# Set default charset
[*]
charset = utf-8

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Trim trailing whitespace
[*]
trim_trailing_whitespace = true

# Use 4 space indentation by default
[*]
indent_style = space
indent_size = 4
